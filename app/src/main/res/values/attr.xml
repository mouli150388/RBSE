<?xml version="1.0" encoding="utf-8"?>
<resources>
    <bool name="is_tablet">false</bool>

    <attr name="sv_speedometerColor" format="color"/>
    <attr name="sv_speedometerBackColor" format="color"/>
    <attr name="sv_centerCircleColor" format="color" />

    <attr name="sv_speedBackgroundColor" format="color" />
    <attr name="sv_withEffects" format="boolean" />

    <attr name="sv_orientation" format="enum" >
        <enum name="HORIZONTAL" value="0" />
        <enum name="VERTICAL" value="1" />
    </attr>

    <attr name="sv_image" format="reference" />

    <declare-styleable name="Gauge">
        <attr name="sv_textColor" format="color" />
        <attr name="sv_speedTextColor" format="color"/>
        <attr name="sv_unitTextColor" format="color"/>
        <attr name="sv_withTremble" format="boolean" />
        <attr name="sv_speedTextSize" format="dimension" />
        <attr name="sv_textSize" format="dimension" />
        <attr name="sv_unit" format="string" />

        <attr name="sv_unitTextSize" format="dimension"/>
        <attr name="sv_trembleDegree" format="float" />
        <attr name="sv_trembleDuration" format="integer" />
        <attr name="sv_lowSpeedPercent" format="integer" />
        <attr name="sv_mediumSpeedPercent" format="integer" />
        <attr name="sv_textRightToLeft" format="boolean" />

        <attr name="sv_minSpeed" format="float" />
        <attr name="sv_maxSpeed" format="float" />

        <attr name="sv_accelerate" format="float" />
        <attr name="sv_decelerate" format="float" />

        <attr name="sv_speedTextTypeface" format="string" />
        <attr name="sv_textTypeface" format="string" />

        <attr name="sv_speedTextPosition" format="enum" >
            <enum name="TOP_LEFT" value="0" />
            <enum name="TOP_CENTER" value="1" />
            <enum name="TOP_RIGHT" value="2" />
            <enum name="LEFT" value="3" />
            <enum name="CENTER" value="4" />
            <enum name="RIGHT" value="5" />
            <enum name="BOTTOM_LEFT" value="6" />
            <enum name="BOTTOM_CENTER" value="7" />
            <enum name="BOTTOM_RIGHT" value="8" />
        </attr>
        <attr name="sv_unitUnderSpeedText" format="boolean" />
        <attr name="sv_unitSpeedInterval" format="dimension" />
        <attr name="sv_speedTextPadding" format="dimension" />
        <attr name="sv_speedTextFormat" format="enum" >
            <enum name="INTEGER" value="0" />
            <enum name="FLOAT" value="1" />
        </attr>
        <attr name="sv_tickTextFormat" format="enum" >
            <enum name="INTEGER" value="0" />
            <enum name="FLOAT" value="1" />
        </attr>

    </declare-styleable>

    <declare-styleable name="Speedometer"  parent="Gauge">
        <attr name="sv_indicatorColor" format="color" />
        <attr name="sv_withIndicatorLight" format="boolean" />
        <attr name="sv_indicatorLightColor" format="color" />
        <attr name="sv_markColor" format="color" />
        <attr name="sv_lowSpeedColor" format="color" />
        <attr name="sv_mediumSpeedColor" format="color" />
        <attr name="sv_highSpeedColor" format="color" />
        <attr name="sv_backgroundCircleColor" format="color" />
        <attr name="sv_speedometerWidth" format="dimension" />

        <attr name="sv_startDegree" format="integer" />
        <attr name="sv_endDegree" format="integer" />

        <attr name="sv_indicatorWidth" format="dimension" />

        <attr name="sv_indicator" format="enum" >
            <enum name="NoIndicator" value="0" />
            <enum name="NormalIndicator" value="1" />
            <enum name="NormalSmallIndicator" value="2" />
            <enum name="TriangleIndicator" value="3" />
            <enum name="SpindleIndicator" value="4" />
            <enum name="LineIndicator" value="5" />
            <enum name="HalfLineIndicator" value="6" />
            <enum name="QuarterLineIndicator" value="7" />
            <enum name="KiteIndicator" value="8" />
            <enum name="NeedleIndicator" value="9" />
        </attr>

        <attr name="sv_speedometerMode" format="enum" >
            <enum name="NORMAL" value="0" />
            <enum name="LEFT" value="1" />
            <enum name="TOP" value="2" />
            <enum name="RIGHT" value="3" />
            <enum name="BOTTOM" value="4" />
            <enum name="TOP_LEFT" value="5" />
            <enum name="TOP_RIGHT" value="6" />
            <enum name="BOTTOM_RIGHT" value="7" />
            <enum name="BOTTOM_LEFT" value="8" />
        </attr>

        <attr name="sv_cutPadding" format="dimension" />

        <attr name="sv_tickNumber" format="integer" />
        <attr name="sv_tickRotation" format="boolean" />
        <attr name="sv_tickPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SpeedView" parent="Speedometer">
        <attr name="sv_centerCircleColor" />
    </declare-styleable>

    <declare-styleable name="DeluxeSpeedView" parent="Speedometer">
        <attr name="sv_centerCircleColor" />
        <attr name="sv_speedBackgroundColor" />
        <attr name="sv_withEffects" />
    </declare-styleable>

    <declare-styleable name="AwesomeSpeedometer" parent="Speedometer">
        <attr name="sv_speedometerColor" />
        <attr name="sv_trianglesColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RaySpeedometer" parent="Speedometer">
        <attr name="sv_speedBackgroundColor" />
        <attr name="sv_withEffects" />
        <attr name="sv_rayColor" format="color"/>
        <attr name="sv_degreeBetweenMark" format="integer"/>
        <attr name="sv_markWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PointerSpeedometer" parent="Speedometer">
        <attr name="sv_centerCircleColor" />
        <attr name="sv_speedometerColor" />
        <attr name="sv_pointerColor" format="color"/>
        <attr name="sv_withPointer" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TubeSpeedometer" parent="Speedometer">
        <attr name="sv_speedometerBackColor" />
        <attr name="sv_withEffects3D" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ImageSpeedometer" parent="Speedometer">
        <attr name="sv_image" />
    </declare-styleable>

    <declare-styleable name="LinearGauge" parent="Gauge">
        <attr name="sv_orientation" />
        <attr name="sv_speedometerColor" />
        <attr name="sv_speedometerBackColor" />
    </declare-styleable>

    <declare-styleable name="ImageLinearGauge" parent="LinearGauge">
        <attr name="sv_speedometerBackColor" />
        <attr name="sv_image" />
    </declare-styleable>



   <!-- //Progress View-->
    <declare-styleable name="ProgressView">
        <attr name="pvDirection" format="enum">
            <enum name="fromLeft" value="0"/>
            <enum name="fromRight" value="1"/>
        </attr>
        <attr name="pvShape" format="enum">
            <enum name="arc" value="0"/>
            <enum name="circle" value="1"/>
            <enum name="line" value="2"/>
        </attr>
        <attr name="pvProgress" format="float" />
        <attr name="pvBackgroundColor" format="color" />
        <attr name="pvProgressColor" format="color" />
        <attr name="pvBackgroundWidth" format="dimension" />
        <attr name="pvProgressWidth" format="dimension" />
        <attr name="pvAnimateDuration" format="integer" />
    </declare-styleable>



<!--Smile RatingBar-->

    <declare-styleable name="SmileRating">
        <attr name="angryColor" format="color" />
        <attr name="normalColor" format="color" />
        <attr name="drawingColor" format="color" />
        <attr name="showLine" format="boolean" />
        <attr name="placeHolderSmileColor" format="color" />
        <attr name="placeHolderBackgroundColor" format="color" />
        <attr name="textSelectionColor" format="color" />
        <attr name="textNonSelectionColor" format="color" />
        <attr name="isIndicator" format="boolean" />
    </declare-styleable>


    <declare-styleable name="CountryCodePicker">
        <attr format="integer" name="ccp_defaultCode"/>
        <attr format="color" name="ccp_textColor"/>
        <attr format="color" name="ccp_backgroundColor"/>
        <attr format="dimension" name="ccp_textSize"/>
        <attr format="string" name="ccp_textFont"/>
        <attr format="dimension" name="ccp_arrowSize"/>
        <attr format="string" name="ccp_countryPreference"/>
        <attr format="string" name="ccp_defaultNameCode"/>
        <attr format="boolean" name="ccp_hideNameCode"/>
        <attr format="boolean" name="ccp_keyboardAutoPopOnSearch"/>
        <attr format="string" name="ccp_customMasterCountries"/>
        <attr format="boolean" name="ccp_showFlag"/>
        <attr format="boolean" name="ccp_showFullName"/>
        <attr format="boolean" name="ccp_selectionDialogShowSearch"/>
        <attr format="boolean" name="ccp_clickable"/>
        <attr format="boolean" name="ccp_hidePhoneCode"/>
        <attr format="boolean" name="ccp_enableHint"/>
        <attr format="boolean" name="ccp_enablePhoneAutoFormatter"/>
        <attr format="boolean" name="ccp_setCountryByTimeZone"/>

        <!--For dialog-->
        <attr format="color" name="ccp_dialogTextColor"/>
    </declare-styleable>


    <declare-styleable name="CircularProgressBarThumb">
        <attr name="stroke_width" format="dimension" />
        <attr name="progress" format="float" />
        <attr name="marker_progress" format="float" />
        <attr name="progress_color" format="color" />
        <attr name="progress_background_color" format="color" />
        <attr name="thumb_visible" format="boolean" />
        <attr name="marker_visible" format="boolean" />
        <attr name="android:gravity" />
    </declare-styleable>

    <attr name="circularProgressBarStyle" format="reference" />


    <!-- **************** Image Crope *************** -->
    <declare-styleable name="CropImageView">
        <attr name="cropGuidelines">
            <enum name="off" value="0"/>
            <enum name="onTouch" value="1"/>
            <enum name="on" value="2"/>
        </attr>
        <attr name="cropScaleType">
            <enum name="fitCenter" value="0"/>
            <enum name="center" value="1"/>
            <enum name="centerCrop" value="2"/>
            <enum name="centerInside" value="3"/>
        </attr>
        <attr name="cropShape">
            <enum name="rectangle" value="0"/>
            <enum name="oval" value="1"/>
        </attr>
        <attr name="cropAutoZoomEnabled" format="boolean"/>
        <attr name="cropMaxZoom" format="integer"/>
        <attr name="cropMultiTouchEnabled" format="boolean"/>
        <attr name="cropFixAspectRatio" format="boolean"/>
        <attr name="cropAspectRatioX" format="integer"/>
        <attr name="cropAspectRatioY" format="integer"/>
        <attr name="cropInitialCropWindowPaddingRatio" format="float"/>
        <attr name="cropBorderLineThickness" format="dimension"/>
        <attr name="cropBorderLineColor" format="color"/>
        <attr name="cropBorderCornerThickness" format="dimension"/>
        <attr name="cropBorderCornerOffset" format="dimension"/>
        <attr name="cropBorderCornerLength" format="dimension"/>
        <attr name="cropBorderCornerColor" format="color"/>
        <attr name="cropGuidelinesThickness" format="dimension"/>
        <attr name="cropGuidelinesColor" format="color"/>
        <attr name="cropBackgroundColor" format="color"/>
        <attr name="cropSnapRadius" format="dimension"/>
        <attr name="cropTouchRadius" format="dimension"/>
        <attr name="cropSaveBitmapToInstanceState" format="boolean"/>
        <attr name="cropShowCropOverlay" format="boolean"/>
        <attr name="cropShowProgressBar" format="boolean"/>
        <attr name="cropMinCropWindowWidth" format="dimension"/>
        <attr name="cropMinCropWindowHeight" format="dimension"/>
        <attr name="cropMinCropResultWidthPX" format="float"/>
        <attr name="cropMinCropResultHeightPX" format="float"/>
        <attr name="cropMaxCropResultWidthPX" format="float"/>
        <attr name="cropMaxCropResultHeightPX" format="float"/>
        <attr name="cropFlipHorizontally" format="boolean"/>
        <attr name="cropFlipVertically" format="boolean"/>
    </declare-styleable>
</resources>